{
  "name": "mat-datetimepicker",
  "version": "2.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "start": "ng serve",
    "start:prod": "ng serve --prod",
    "build": "yarn build:lib && ng build",
    "build:prod": "yarn build:lib && ng build --prod --stats-json",
    "build:lib": "yarn build:lib/core && yarn build:lib/moment",
    "build:lib/core": "ng build core && copy-files-from-to --config projects/core/copy-files-from-to.json",
    "build:lib/moment": "ng build moment && copy-files-from-to --config projects/moment/copy-files-from-to.json",
    "bundle-report": "webpack-bundle-analyzer dist/app/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.0-rc.0",
    "@angular/cdk": "~9.0.0-rc.0",
    "@angular/common": "^9.0.0-rc.0",
    "@angular/compiler": "^9.0.0-rc.0",
    "@angular/core": "^9.0.0-rc.0",
    "@angular/forms": "^9.0.0-rc.0",
    "@angular/material": "~9.0.0-rc.0",
    "@angular/material-moment-adapter": "~9.0.0-rc.0",
    "@angular/platform-browser": "^9.0.0-rc.0",
    "@angular/platform-browser-dynamic": "^9.0.0-rc.0",
    "@angular/router": "^9.0.0-rc.0",
    "copy-files-from-to": "2.1.0",
    "moment": "2.24.0",
    "rxjs": "~6.5.2",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.0-rc.0",
    "@angular-devkit/build-ng-packagr": "~0.900.0-rc.0",
    "@angular/cli": "^9.0.0-rc.0",
    "@angular/compiler-cli": "^9.0.0-rc.0",
    "@angular/language-service": "^9.0.0-rc.0",
    "@types/jasmine": "^3.3.16",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^9.0.0-rc.1",
    "protractor": "^5.4.2",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.6.4",
    "webpack-bundle-analyzer": "3.6.0"
  }
}
